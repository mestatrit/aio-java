<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cdpc.aio.web.po">
    <class name="TblSysUsrinf" table="tbl_sys_usrinf">
        <id name="uiId" column="UI_ID" type="long">
            <generator class="assigned"/>
        </id>
		<property name="uiUserId" column="UI_USER_ID" type="string" not-null="false" unique="false" length="20" update="true" insert="true"/>
		<property name="uiUserPwd" column="UI_USER_PWD" type="string" not-null="false" unique="false" length="50" update="true" insert="true"/>
		<property name="uiLstIpAddress" column="UI_LST_IP_ADDRESS" type="string" not-null="false" unique="false" length="20" update="true" insert="true"/>
		<property name="uiLstUserLoginTime" column="UI_LST_USER_LOGIN_TIME" type="string" not-null="false" unique="false" length="19" update="true" insert="true"/>
		<property name="uiCurLogStats" column="UI_CUR_LOG_STATS" type="string" not-null="false" unique="false" length="1" update="true" insert="true"/>
		<property name="uiLstModiUserId" column="UI_LST_MODI_USER_ID" type="string" not-null="false" unique="false" length="20" update="true" insert="true"/>
		<property name="uiLstModiDate" column="UI_LST_MODI_DATE" type="string" not-null="false" unique="false" length="19" update="true" insert="true"/>
    </class>
</hibernate-mapping>

