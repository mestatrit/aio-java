<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cdpc.aio.web.po">
    <class name="TblSysLogrec" table="TBL_SYS_LOGREC">
        <id name="lrId" column="LR_ID" type="long">
            <generator class="assigned"/>
        </id>
		<property name="lrUsername" column="LR_USERNAME" type="string" not-null="false" unique="false" length="10" update="true" insert="true"/>
		<property name="lrLogindate" column="LR_LOGINDATE" type="string" not-null="false" unique="false" length="20" update="true" insert="true"/>
		<property name="lrLoginip" column="LR_LOGINIP" type="string" not-null="false" unique="false" length="20" update="true" insert="true"/>
    </class>
</hibernate-mapping>

